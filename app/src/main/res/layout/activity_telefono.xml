<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".telefonoActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="215dp"
        android:layout_height="206dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="4dp"
        android:contentDescription="@string/foto_app"
        android:theme="@style/Widget.AppCompat.Button.Colored"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logoconfondoapp"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="218dp"
        android:layout_height="55dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="348dp"
        android:text="@string/cabeceratelefono"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="218dp"
        android:layout_height="55dp"
        android:layout_marginStart="96dp"
        android:layout_marginLeft="284dp"
        android:layout_marginTop="460dp"
        android:text="@string/ntelefonoes"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonSelTel"
        android:layout_width="220dp"
        android:layout_height="90dp"
        android:layout_marginStart="92dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="396dp"
        android:text="@string/selBoton"
        android:theme="@android:color/holo_green_dark"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:autofillHints="phone"
        android:id="@+id/NumTel"
        android:inputType="none"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="528dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
         />

    <Button
        android:id="@+id/buttonContinuar"
        android:layout_width="220dp"
        android:layout_height="90dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="596dp"
        android:text="@string/continuar"
        android:theme="@android:color/holo_green_light"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>